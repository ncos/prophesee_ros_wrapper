<?xml version="1.0"?>

<launch>
  <arg name="bias_file" default="$(find prophesee_ros_driver)/launch/config/hf_60.bias"/>
  <node pkg="prophesee_ros_driver" type="prophesee_ros_publisher" name="prophesee_publisher_hvga" respawn="false" output="screen" required="true">
    <param name="camera_name" value="hvga" />
    <param name="camera_serial" type="string" value="0000ffff" />
    <param name="publish_cd" value="true" />
    <param name="publish_graylevels" value="true" />
    <param name="publish_imu" value="true" />

    <!-- Path to the file with the camera settings -->
    <param name="bias_file" value="$(arg bias_file)" />

    <!-- Maximum event rate in kEv/s -->
    <param name="max_event_rate" value="4000" />

    <!-- Graylevel frame rate in fps -->
    <param name="graylevel_frame_rate" value="40" />
  </node>
</launch>
