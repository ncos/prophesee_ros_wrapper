<?xml version="1.0"?>

<launch>
  <node pkg="prophesee_ros_driver" type="prophesee_ros_publisher" name="prophesee_publisher_right" respawn="false" output="screen" required="true">
    <param name="camera_name" value="right" />
    <param name="camera_serial" type="string" value="00006697" />
    <param name="publish_cd" value="true" />
    <param name="publish_graylevels" value="false" />
    <param name="publish_imu" value="true" />

    <!-- Path to the file with the camera settings -->
    <param name="bias_file" value="" />

    <!-- Maximum event rate in kEv/s -->
    <param name="max_event_rate" value="10000" />

    <!-- Graylevel frame rate in fps -->
    <param name="graylevel_frame_rate" value="30" />
  </node>
</launch>
